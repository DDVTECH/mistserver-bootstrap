{
  "uid": "mist-overview",
  "title": "MistServer Overview",
  "timezone": "browser",
  "editable": true,
  "schemaVersion": 39,
  "version": 2,
  "refresh": "30s",
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "timepicker": {},
  "templating": {
    "list": []
  },
  "panels": [
    {
      "id": 1,
      "title": "Mist Target Up",
      "type": "stat",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "refId": "A",
          "expr": "max(up{job=\"mist\"})"
        }
      ],
      "options": {
        "orientation": "auto",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "center",
        "reduceOptions": {
          "values": false,
          "calcs": [
            "lastNotNull"
          ],
          "fields": ""
        }
      },
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {
              "type": "special",
              "match": "null",
              "result": {
                "text": "N/A"
              }
            },
            {
              "type": "value",
              "options": {
                "1": {
                  "text": "UP"
                },
                "0": {
                  "text": "DOWN"
                }
              }
            }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "red",
                "value": 0
              },
              {
                "color": "green",
                "value": 1
              }
            ]
          },
          "color": {
            "mode": "thresholds"
          }
        },
        "overrides": []
      }
    },
    {
      "id": 2,
      "title": "CPU Usage",
      "type": "stat",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 6,
        "y": 0
      },
      "targets": [
        {
          "refId": "A",
          "expr": "avg(mist_cpu{job=\"mist\"})/10"
        }
      ],
      "options": {
        "orientation": "auto",
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "center",
        "reduceOptions": {
          "values": false,
          "calcs": [
            "lastNotNull"
          ],
          "fields": ""
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 1,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": 0
              },
              {
                "color": "orange",
                "value": 70
              },
              {
                "color": "red",
                "value": 90
              }
            ]
          },
          "mappings": []
        },
        "overrides": []
      }
    },
    {
      "id": 3,
      "title": "Memory Usage",
      "type": "stat",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 12,
        "y": 0
      },
      "targets": [
        {
          "refId": "A",
          "expr": "avg((mist_mem_used{job=\"mist\"}+mist_shm_used{job=\"mist\"}) / mist_mem_total{job=\"mist\"}) * 100"
        }
      ],
      "options": {
        "orientation": "auto",
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "center",
        "reduceOptions": {
          "values": false,
          "calcs": [
            "lastNotNull"
          ],
          "fields": ""
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 1,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": 0
              },
              {
                "color": "orange",
                "value": 70
              },
              {
                "color": "red",
                "value": 85
              }
            ]
          },
          "mappings": []
        },
        "overrides": []
      }
    },
    {
      "id": 4,
      "title": "Active Viewers",
      "type": "stat",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 18,
        "y": 0
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(mist_sessions_total{job=\"mist\",sessType=\"viewers\"})"
        }
      ],
      "options": {
        "orientation": "auto",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "center",
        "reduceOptions": {
          "values": false,
          "calcs": [
            "lastNotNull"
          ],
          "fields": ""
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "decimals": 0,
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": 0
              }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "id": 5,
      "title": "Sessions by State",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 4
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "unit": "short"
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "multi",
          "sort": "desc"
        }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "mist_sessions_total{job=\"mist\"}",
          "legendFormat": "{{sessType}}"
        }
      ]
    },
    {
      "id": 6,
      "title": "Bandwidth",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 4
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "unit": "bps"
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "multi",
          "sort": "desc"
        }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "irate(mist_bw_total{job=\"mist\",direction=\"down\"}[1m]) * 8",
          "legendFormat": "Download"
        },
        {
          "refId": "B",
          "expr": "irate(mist_bw_total{job=\"mist\",direction=\"up\"}[1m]) * 8",
          "legendFormat": "Upload"
        }
      ]
    },
    {
      "id": 7,
      "title": "Outputs by Protocol",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 12
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "unit": "short"
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "right"
        },
        "tooltip": {
          "mode": "multi",
          "sort": "desc"
        }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(mist_outputs{job=\"mist\"}) by (output)",
          "legendFormat": "{{output}}"
        }
      ]
    },
    {
      "id": 8,
      "title": "Trigger Failure Rate",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 12
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "unit": "percent"
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "multi",
          "sort": "desc"
        }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "rate(mist_trigger_fails{job=\"mist\"}[15m]) / on(trigger) rate(mist_trigger_count{job=\"mist\"}[15m])",
          "legendFormat": "{{trigger}}"
        }
      ]
    },
    {
      "id": 9,
      "title": "Sessions & Logs per Minute",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 20
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "unit": "short"
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "multi",
          "sort": "desc"
        }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "increase(mist_sessions_count{job=\"mist\"}[1m])",
          "legendFormat": "{{sessType}} sessions"
        },
        {
          "refId": "B",
          "expr": "increase(mist_logs{job=\"mist\"}[1m])",
          "legendFormat": "Logs"
        }
      ]
    },
    {
      "id": 10,
      "title": "Trigger Runtime",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 20
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "unit": "ms"
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "multi",
          "sort": "desc"
        }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "rate(mist_trigger_time{job=\"mist\"}[15m]) / on(trigger) rate(mist_trigger_count{job=\"mist\"}[15m])",
          "legendFormat": "{{trigger}}"
        }
      ]
    }
  ]
}